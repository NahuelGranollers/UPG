{
  "name": "upg-community-hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "vite",
    "build": "vite build && npm run copy-404",
    "copy-404": "node -e \"const fs = require('fs'); if (fs.existsSync('404.html') && fs.existsSync('dist/')) { fs.copyFileSync('404.html', 'dist/404.html'); console.log('404.html copied successfully'); } else { console.log('404.html not found or dist/ does not exist'); }\"",
    "copy-dist": "node -e \"require('fs').cpSync('dist/index.html', 'index.html'); require('fs').cpSync('dist/assets', 'assets', {recursive: true})\"",
    "copy-server": "node -e \"const fs = require('fs'); fs.cpSync('server', 'C:/Users/nahue/Documents/GitHub/Mensajeria', {recursive: true}); fs.copyFileSync('package.json', 'C:/Users/nahue/Documents/GitHub/Mensajeria/package.json');\"",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@tailwindcss/vite": "^4.1.17",
    "axios": "^1.13.2",
    "babylonjs": "^8.38.0",
