    "babylonjs-loaders": "^8.39.0",
    "babylonjs-materials": "^8.39.0",
    "better-sqlite3": "^12.4.5",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "firebase": "^10.14.1",
    "helmet": "^8.1.0",
    "lucide-react": "^0.554.0",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.4.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "xss": "^1.0.15"
  },
